#include <stdio.h>
#include <stdlib.h>

int main() {
    int n, q;
    scanf("%d %d", &n, &q);

    // Create an array of pointers (each will act as a dynamic sequence)
    int **seqList = (int **)malloc(n * sizeof(int *));
    int *sizes = (int *)calloc(n, sizeof(int));  // stores size of each sequence

    for (int i = 0; i < n; i++) {
        seqList[i] = NULL;  // initially no elements
    }

    int lastAnswer = 0;

    while (q--) {
        int type, x, y;
        scanf("%d %d %d", &type, &x, &y);
        
        int idx = (x ^ lastAnswer) % n;

        if (type == 1) {
            // append y to sequence idx
            sizes[idx]++;
            seqList[idx] = (int *)realloc(seqList[idx], sizes[idx] * sizeof(int));
            seqList[idx][sizes[idx] - 1] = y;
        } 
        else if (type == 2) {
            // get element y%size and update lastAnswer
            lastAnswer = seqList[idx][y % sizes[idx]];
            printf("%d\n", lastAnswer);
        }
    }

    return 0;
}
